{
  "name": "rocket-admin-panel",
  "version": "1.0.0",
  "private": true,
  "description": "Rocket PC管理端 - Vue3 + Element Plus + TypeScript",
  "type": "module",
  "scripts": {
    "predev": "node ../../scripts/enforce-single-platform.js check admin",
    "dev": "vite",
    "prebuild": "node ../../scripts/enforce-single-platform.js check admin",
    "build": "vite build",
    "build-check": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .vue,.js,.ts,.tsx --fix",
    "type-check": "vue-tsc --noEmit",
    "platform:use": "node ../../scripts/enforce-single-platform.js switch admin"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.0",
    "axios": "^1.6.0",
    "dayjs": "^1.11.0",
    "echarts": "^5.4.0",
    "element-plus": "^2.4.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.0",
    "vue": "^3.4.0",
    "vue-echarts": "^6.6.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@types/nprogress": "^0.2.3",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.0",
    "sass": "^1.69.0",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^0.17.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.0",
    "vue-tsc": "^3.0.6"
  },
  "browserslist": [
    "Chrome >= 80",
    "Firefox >= 78",
    "Safari >= 14",
    "Edge >= 80"
  ]
}
