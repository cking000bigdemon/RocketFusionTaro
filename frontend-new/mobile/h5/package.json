{
  "name": "rocket-mobile-h5",
  "version": "1.0.0",
  "private": true,
  "description": "Rocket移动端H5应用 - Vue3 + Vant",
  "scripts": {
    "predev": "node ../../../scripts/enforce-single-platform.js check h5",
    "dev": "vite",
    "prebuild": "node ../../../scripts/enforce-single-platform.js check h5",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .vue,.js,.ts --fix",
    "type-check": "vue-tsc --noEmit",
    "platform:use": "node ../../../scripts/enforce-single-platform.js switch h5"
  },
  "dependencies": {
    "vue": "^3.3.0",
    "vue-router": "^4.2.0",
    "pinia": "^2.1.0",
    "vant": "^4.6.0",
    "@vant/touch-emulator": "^1.4.0",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.40.0",
    "eslint-plugin-vue": "^9.14.0",
    "postcss": "^8.4.24",
    "postcss-px-to-viewport": "^1.1.1",
    "sass": "^1.63.0",
    "typescript": "^5.0.0",
    "unplugin-vue-components": "^0.25.0",
    "vite": "^4.3.0",
    "vue-tsc": "^1.6.0"
  },
  "browserslist": [
    "Android >= 5.0",
    "iOS >= 10.0",
    "Chrome >= 60"
  ]
}